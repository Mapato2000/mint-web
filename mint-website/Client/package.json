{
  "name": "quasar_template",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "Quasar App",
  "author": "Centre Easy",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "execute": "quasar dev",
    "dev-electron": "quasar dev -m electron",
    "dev-ssr": "quasar dev -m ssr",
    "dev-pwa": "quasar dev -m pwa",
    "dev-android": "quasar dev --mode cordova -T android",
    "dev-ios": "quasar dev --mode cordova -T ios",
    "build-electron": "quasar build -m electron",
    "build-ssr": "quasar build -m ssr",
    "build-pwa": "quasar build -m pwa",
    "build-android": "quasar build -m android",
    "build-ios": "quasar build -m ios"
  },
  "dependencies": {
    "@quasar/extras": "1.13.6",
    "axios": "0.27.2",
    "core-js": "3.22.5",
    "quasar": "2.6.6",
    "vue": "3.2.33"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@quasar/app": "3.3.3",
    "cordova": "11.0.0",
    "electron": "18.2.3",
    "electron-packager": "15.5.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-vue": "8.7.1",
    "eslint-webpack-plugin": "3.1.1",
    "workbox-webpack-plugin": "6.5.3"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
